{% set page = _Page.getPageData(slug) %}

{{ set(this, 'title', page.title | raw) }}

<!-- Begin Container -->
<div class="page_section" style="height:100%">
    <section class="container-fluid" style="height:100%">
        
        <!-- Begin Terms -->
        <div class="content-wrap" style="height:100%">
            <div class="row" style="height:100%">
                <div class="col-xs-12" style="height:100%">
                    <div class="content_area is-wrapper">
                        {{page.content | raw}}                        
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>